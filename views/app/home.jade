            .col-md-3.dropdown
              br
              br
              button.btn.btn-primary.dropdown-toggle(type='button', data-toggle='dropdown')
                span.glyphicon.glyphicon-cog(aria-hidden='true')
              ul.dropdown-menu
                li.text-center(style="background-color:#A9F5A9")
                  strong.text-center
                    a(href='#') #{nombre}
                li
                  a(href='app/configuracion') Configuracion
                li
                  a(href='/') Salir
            .col-md-1
          .row.text-center
            .col-md-1
            #div_configuracion.col-md-10(style='padding-left: 2px;')
              strong Caracteristicas del Curso
            .col-md-1
