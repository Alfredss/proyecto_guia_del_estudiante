          .row
            #myRed.col-md-10.base_malla
            #div_leyenda.col-md-2.base_leyenda.bordes-top
              strong
                p.letra_Patua_One Aprobados
              canvas#aprobado(width='80', height='50', style='background-color:#ffffff')
              script.
                var canvas = document.getElementById('aprobado');
                var context = canvas.getContext('2d');
                context.beginPath();
                context.rect(10, 10, 60, 30);
                context.fillStyle = '#81BEF7';
                context.fill();
                context.lineWidth = 1;
                context.strokeStyle = 'black';
                context.stroke();
              strong
                p.letra_Patua_One No Aprobados
              canvas#noAprobado(width='80', height='50', style='background-color:#ffffff')
              script.
                var canvas = document.getElementById('noAprobado');
                var context = canvas.getContext('2d');
                context.beginPath();
                context.rect(10, 10, 60, 30);
                context.fillStyle = 'gray';
                context.fill();
                context.lineWidth = 1;
                context.strokeStyle = 'black';
                context.stroke();
              strong
                p.letra_Patua_One puedes llevar
              canvas#puedeLlevar(width='80', height='50', style='background-color:#ffffff')
              script.
                var canvas = document.getElementById('puedeLlevar');
                var context = canvas.getContext('2d');
                context.beginPath();
                context.rect(10, 10, 60, 30);
                context.fillStyle = 'white';
                context.fill();
                context.lineWidth = 1;
                context.strokeStyle = 'black';
                context.stroke();
              strong
                p.letra_Patua_One cursos ingles
              canvas#ingles(width='80', height='50', style='background-color:#ffffff')
              script.
                var canvas = document.getElementById('ingles');
                var context = canvas.getContext('2d');
                context.beginPath();
                context.rect(10, 10, 60, 30);
                context.fillStyle = 'yellow';
                context.fill();
                context.lineWidth = 1;
                context.strokeStyle = 'black';
                context.stroke();
