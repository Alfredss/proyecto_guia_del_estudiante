  body(background='img/fondo.png')
  
    .container
      // CABECERA
      header.base_cabecera.row
        .row
          .col-xs-8.col-md-3
            a(href='http://www.unsch.edu.pe/', target='_self ')
              img.mediano(src='img/logo_unsch.png')
          .cols-xs-1.col-md-6.text-center
            h1.letra_Patua_One Guia Experta del Estudiante
          .col-xs-8.col-md-3
            a(href='https://www.facebook.com/groups/1425237611081130/?fref=ts', target='_self ')
              img.mediano.derecha(src='img/logo_is.jpg')
      // Navegacion
      nav.base_navegacion.text-center.row
        .container
          ul(style='list-style:none; padding:0;')
            li.col-md-4
              a(href='/app', style='text-decoration: none; color:#FFFFFF;')
                h4.letra_mediano
                  span.glyphicon.glyphicon-home(aria-hidden='true')
                  |  Home
            li.col-md-4
              a(href='app/estadisticas', style='text-decoration: none; color:#FFFFFF;')
                h4.letra_mediano
                  span.glyphicon.glyphicon-signal(aria-hidden='true')
                  |  Estadisticas
            li.col-md-4
              a(href='app/blog', style='text-decoration: none; color:#FFFFFF;')
                h4.letra_mediano
                  span.glyphicon.glyphicon-file(aria-hidden='true')
                  |  Blog
          // Trigger the modal with a button
      // Contenido
      #cuerpo.row
        .col-md-2.base_perfil
          #div_titulo_configuracion.row.base_serie.text-center.bordes-sin-derecha(style='padding-top:1px')
